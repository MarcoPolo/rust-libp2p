{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "~/.cargo/bin/cargo",
			"args": [
				"test"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "libp2p - cargo test"
		},
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build - rust-libp2p"
		}
	]
}